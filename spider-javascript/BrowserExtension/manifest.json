{

  "manifest_version": 2,
  "name": "MyWebExt",
  "version": "1.0",

  "description": "such extension, much automation, wow",

  "icons": {
  },

  "content_scripts": [
    {
      "matches": ["*://*.taobao.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "page": "background-page.html"
  },
  "permissions": [
    "*://127.0.0.1:8710/*"]
}